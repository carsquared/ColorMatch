<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="carsquared.colormatch">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <meta-data
            android:name="com.google.android.gms.games.APP_ID"
            android:value="@string/app_id" />
        <meta-data
            android:name="com.google.android.gms.version"
            android:value="@integer/google_play_services_version" />


        <activity android:name=".MainActivity" />
        <activity android:name=".gamemodes" />
        <activity android:name=".LevelActivity" />
        <activity android:name=".classicwin" />
        <activity android:name=".creditsactivity" />
        <activity android:name=".gameplay" />
        <activity android:name=".infinite" />
        <activity android:name=".instructionsactivity" />
        <activity android:name=".loseactivity" />
        <activity android:name=".options_dialog_Activity" />
        <activity android:name=".speedrun" />
        <activity android:name=".winothergamemodes" />
        <activity android:name=".winroundactivity">

        </activity>
        <activity android:name=".Opening">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>